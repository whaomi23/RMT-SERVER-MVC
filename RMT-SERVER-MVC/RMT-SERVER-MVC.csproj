<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>RMT_SERVER_MVC</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="ClientForServer\**" />
    <Content Remove="ClientForServer\**" />
    <EmbeddedResource Remove="ClientForServer\**" />
    <None Remove="ClientForServer\**" />
  </ItemGroup>

  <ItemGroup>
    <Content Remove="Properties\launchSettings - Copia.json" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Properties\launchSettings - Copia.json" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.Mvc" Version="5.3.0" />
    <PackageReference Include="Microsoft.AspNetCore.SignalR" Version="1.2.0" />
  </ItemGroup>

</Project>
